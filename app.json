{
  "expo": {
    "name": "paes-dictionary",
    "slug": "paes-dictionary",
    "version": "1.0.0",
    "scheme": "paes-dictionary",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.paes.dictionary",
      "versionCode": 1
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dc5e77ed-c8f3-4781-aa79-7435187d8856"
      }
    }
  }
}
